    "@reduxjs/toolkit": "^2.9.0",
    "chroma-js": "^3.1.2",
    "framer-motion": "^12.23.12",
    "localforage": "^1.10.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
